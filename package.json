{
  "name": "raiden-webui",
  "version": "1.1.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve",
    "serve": "ng serve --proxy-config proxy.config.json --base-href /ui/ --deploy-url /ui/",
    "serve:all": "ng serve --proxy-config proxy.config.json --base-href /ui/ --deploy-url /ui/ --host 0.0.0.0",
    "build": "npm run build:prod",
    "build:dev": "ng build",
    "build:prod": "ng build --prod --aot",
    "test": "ng test",
    "lint": "ng lint",
    "prettier": "prettier --write --config ./.prettierrc './**/*.{ts,tsx,html,css,scss,json,js}'",
    "prettier:check": "prettier --check --config ./.prettierrc './**/*.{ts,tsx,html,css,scss,json,js}'",
    "e2e": "ng e2e",
    "postinstall": "ngcc --tsconfig ./src/tsconfig.app.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.0.4",
    "@angular/cdk": "11.0.2",
    "@angular/common": "11.0.4",
    "@angular/compiler": "11.0.4",
    "@angular/core": "11.0.4",
    "@angular/flex-layout": "11.0.0-beta.33",
    "@angular/forms": "11.0.4",
    "@angular/material": "11.0.2",
    "@angular/platform-browser": "11.0.4",
    "@angular/platform-browser-dynamic": "11.0.4",
    "@angular/router": "11.0.4",
    "ajv": "6.12.6",
    "bignumber.js": "9.0.1",
    "core-js": "3.6.5",
    "ethereum-blockies-base64": "1.0.2",
    "jasmine": "3.6.3",
    "material-design-icons": "3.0.1",
    "ngx-clipboard": "14.0.1",
    "ngx-toastr": "13.2.0",
    "qrcode": "1.4.4",
    "rxjs": "6.6.3",
    "tslib": "^2.0.0",
    "web3": "1.3.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1100.4",
    "@angular/cli": "11.0.4",
    "@angular/compiler-cli": "11.0.4",
    "@angular/language-service": "11.0.4",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "14.14.6",
    "@types/qrcode": "1.3.5",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-junit-reporter": "2.0.1",
    "prettier": "2.1.2",
    "protractor": "7.0.0",
    "rxjs-tslint": "0.1.8",
    "sass": "^1.30.0",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "4.0.5"
  }
}
