{
  "name": "raiden-webui",
  "version": "0.9.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve",
    "serve": "ng serve --proxy-config proxy.config.json --base-href /ui/ --deploy-url /ui/",
    "serve:all": "ng serve --proxy-config proxy.config.json --base-href /ui/ --deploy-url /ui/ --host 0.0.0.0",
    "build": "npm run build:prod",
    "build:dev": "ng build",
    "build:prod": "ng build --prod --aot",
    "test": "ng test",
    "lint": "ng lint",
    "prettier": "prettier --write --config ./.prettierrc './**/*.{ts,tsx,html,css,scss,json,js}'",
    "prettier:check": "prettier --check --config ./.prettierrc './**/*.{ts,tsx,html,css,scss,json,js}'",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.7",
    "@angular/cdk": "^7.3.3",
    "@angular/common": "^7.2.7",
    "@angular/compiler": "^7.2.7",
    "@angular/core": "^7.2.7",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "^7.2.7",
    "@angular/http": "^7.2.7",
    "@angular/material": "^7.3.3",
    "@angular/platform-browser": "^7.2.7",
    "@angular/platform-browser-dynamic": "^7.2.7",
    "@angular/router": "^7.2.7",
    "ajv": "^6.6.2",
    "bignumber.js": "^8.0.1",
    "core-js": "^2.6.1",
    "ethereum-blockies-base64": "^1.0.2",
    "hammerjs": "^2.0.8",
    "jasmine": "^3.3.1",
    "material-design-icons": "^3.0.1",
    "ngx-clipboard": "11.1.9",
    "ngx-toastr": "^9.1.1",
    "rxjs": "^6.4.0",
    "tslib": "^1.9.0",
    "web3": "^1.0.0-beta.55",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.1",
    "@angular/cli": "7.3.4",
    "@angular/compiler-cli": "^7.2.7",
    "@angular/language-service": "^7.2.7",
    "@types/jasmine": "~3.3.4",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^10.7.1",
    "codelyzer": "^4.5.0",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-junit-reporter": "^1.2.0",
    "node-sass": "^4.12.0",
    "prettier": "1.16.1",
    "protractor": "~5.4.1",
    "rxjs-tslint": "^0.1.5",
    "ts-node": "~7.0.1",
    "tslint": "^5.12.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "3.2.4"
  }
}
