{
  "name": "raiden-webui",
  "version": "1.0.2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve",
    "serve": "ng serve --proxy-config proxy.config.json --base-href /ui/ --deploy-url /ui/",
    "serve:all": "ng serve --proxy-config proxy.config.json --base-href /ui/ --deploy-url /ui/ --host 0.0.0.0",
    "build": "npm run build:prod",
    "build:dev": "ng build",
    "build:prod": "ng build --prod --aot",
    "test": "ng test",
    "lint": "ng lint",
    "prettier": "prettier --write --config ./.prettierrc './**/*.{ts,tsx,html,css,scss,json,js}'",
    "prettier:check": "prettier --check --config ./.prettierrc './**/*.{ts,tsx,html,css,scss,json,js}'",
    "e2e": "ng e2e",
    "postinstall": "ngcc --tsconfig ./src/tsconfig.app.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.0.7",
    "@angular/cdk": "10.1.1",
    "@angular/common": "10.0.7",
    "@angular/compiler": "10.0.7",
    "@angular/core": "10.0.7",
    "@angular/flex-layout": "10.0.0-beta.32",
    "@angular/forms": "10.0.7",
    "@angular/material": "10.1.1",
    "@angular/platform-browser": "10.0.7",
    "@angular/platform-browser-dynamic": "10.0.7",
    "@angular/router": "10.0.7",
    "ajv": "6.12.3",
    "bignumber.js": "9.0.0",
    "core-js": "3.6.5",
    "ethereum-blockies-base64": "1.0.2",
    "jasmine": "3.6.1",
    "material-design-icons": "3.0.1",
    "ngx-clipboard": "13.0.1",
    "ngx-toastr": "13.0.0",
    "qrcode": "1.4.4",
    "rxjs": "6.6.2",
    "tslib": "2.0.0",
    "web3": "1.2.11",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1000.5",
    "@angular/cli": "10.0.5",
    "@angular/compiler-cli": "10.0.7",
    "@angular/language-service": "10.0.7",
    "@types/jasmine": "3.5.12",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "14.0.27",
    "@types/qrcode": "1.3.5",
    "codelyzer": "6.0.0",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.1.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "3.3.1",
    "karma-junit-reporter": "2.0.1",
    "node-sass": "4.14.1",
    "prettier": "2.0.5",
    "protractor": "7.0.0",
    "rxjs-tslint": "0.1.8",
    "ts-node": "8.10.2",
    "tslint": "6.1.3",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "3.9.7"
  }
}
