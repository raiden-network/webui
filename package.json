{
  "name": "raiden-webui",
  "version": "0.20.0-alpha2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve",
    "serve": "ng serve --proxy-config proxy.config.json --base-href /ui/ --deploy-url /ui/",
    "serve:all": "ng serve --proxy-config proxy.config.json --base-href /ui/ --deploy-url /ui/ --host 0.0.0.0",
    "build": "npm run build:prod",
    "build:dev": "ng build",
    "build:prod": "ng build --prod --aot",
    "test": "ng test",
    "lint": "ng lint",
    "prettier": "prettier --write --config ./.prettierrc './**/*.{ts,tsx,html,css,scss,json,js}'",
    "prettier:check": "prettier --check --config ./.prettierrc './**/*.{ts,tsx,html,css,scss,json,js}'",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.1",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "^9.0.1",
    "@angular/compiler": "^9.0.1",
    "@angular/core": "^9.0.1",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "^9.0.1",
    "@angular/material": "^9.0.0",
    "@angular/platform-browser": "^9.0.1",
    "@angular/platform-browser-dynamic": "^9.0.1",
    "@angular/router": "^9.0.1",
    "ajv": "^6.12.0",
    "bignumber.js": "^9.0.0",
    "core-js": "^3.6.5",
    "ethereum-blockies-base64": "^1.0.2",
    "jasmine": "^3.5.0",
    "material-design-icons": "^3.0.1",
    "ngx-clipboard": "13.0.0",
    "ngx-toastr": "^12.0.1",
    "qrcode": "^1.4.4",
    "rxjs": "^6.5.5",
    "tslib": "^1.11.1",
    "web3": "1.2.6",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular/cli": "9.1.3",
    "@angular/compiler-cli": "^9.0.1",
    "@angular/language-service": "^9.1.3",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^13.11.1",
    "@types/qrcode": "^1.3.4",
    "codelyzer": "^5.2.2",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "^5.0.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.1.1",
    "karma-junit-reporter": "^2.0.1",
    "node-sass": "^4.13.1",
    "prettier": "2.0.5",
    "protractor": "^5.4.4",
    "rxjs-tslint": "^0.1.8",
    "ts-node": "~8.8.2",
    "tslint": "^5.20.1",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "3.7.5"
  }
}
