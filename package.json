{
  "name": "raiden-webui",
  "version": "1.1.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve",
    "serve": "ng serve --proxy-config proxy.config.json --base-href /ui/ --deploy-url /ui/",
    "serve:all": "ng serve --proxy-config proxy.config.json --base-href /ui/ --deploy-url /ui/ --host 0.0.0.0",
    "build": "npm run build:prod",
    "build:dev": "ng build",
    "build:prod": "ng build --prod --aot",
    "test": "ng test",
    "lint": "ng lint raidenwebui",
    "prettier": "prettier --write --config ./.prettierrc './**/*.{ts,tsx,html,css,scss,json,js}'",
    "prettier:check": "prettier --check --config ./.prettierrc './**/*.{ts,tsx,html,css,scss,json,js}'",
    "e2e": "ng e2e",
    "postinstall": "ngcc --tsconfig ./src/tsconfig.app.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.1.2",
    "@angular/cdk": "11.1.2",
    "@angular/common": "11.1.2",
    "@angular/compiler": "11.1.2",
    "@angular/core": "11.1.2",
    "@angular/flex-layout": "11.0.0-beta.33",
    "@angular/forms": "11.1.2",
    "@angular/material": "11.1.2",
    "@angular/platform-browser": "11.1.2",
    "@angular/platform-browser-dynamic": "11.1.2",
    "@angular/router": "11.1.2",
    "ajv": "6.12.6",
    "bignumber.js": "9.0.1",
    "core-js": "3.8.3",
    "ethereum-blockies-base64": "1.0.2",
    "jasmine": "3.6.4",
    "material-design-icons": "3.0.1",
    "ngx-clipboard": "14.0.1",
    "ngx-toastr": "13.2.0",
    "qrcode": "1.4.4",
    "rxjs": "6.6.3",
    "tslib": "2.0.3",
    "web3": "1.3.4",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "10.0.1",
    "@angular-devkit/build-angular": "0.1101.4",
    "@angular-eslint/builder": "0.8.0-beta.7",
    "@angular-eslint/eslint-plugin": "0.8.0-beta.7",
    "@angular-eslint/eslint-plugin-template": "0.8.0-beta.7",
    "@angular-eslint/schematics": "0.8.0-beta.6",
    "@angular-eslint/template-parser": "0.8.0-beta.7",
    "@angular/cli": "11.1.4",
    "@angular/compiler-cli": "11.1.2",
    "@angular/language-service": "11.1.2",
    "@types/jasmine": "3.6.1",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "14.14.25",
    "@types/qrcode": "1.4.0",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "codelyzer": "6.0.1",
    "eslint": "7.15.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.13",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.2",
